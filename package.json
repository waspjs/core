  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index",
    "start:dev": "tsc-watch --onSuccess \"yarn start\"",
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "mocha dist/**/*.spec.js",
    "test:dev": "tsc-watch --onSuccess \"yarn test\"",
    "generate": "graphql-codegen --config codegen.graphql.yml",
    "js": "yarn --cwd ./src/views/js",
    "prepublishOnly": "yarn build",
    "postinstall": "yarn js install"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "dependencies": {
    "@typegoose/typegoose": "6.1.5",
    "apollo-server-express": "2.9.9",
    "bcrypt": "3.0.6",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-type-datetime": "0.2.4",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mongoose": "5.7.11",
    "randomstring": "1.1.5",
    "recursive-readdir": "2.2.2",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.16",
    "stack-trace": "0.0.10",
    "typed-html": "2.0.0",
    "typedi": "0.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-operations": "1.8.3",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.2",
    "@types/jsonwebtoken": "8.3.5",
    "@types/lodash": "4.14.149",
    "@types/mocha": "5.2.7",
    "@types/mongoose": "5.5.32",
    "@types/node": "12.12.9",
    "@types/randomstring": "1.1.6",
    "@types/recursive-readdir": "2.2.0",
    "@types/source-map-support": "0.5.0",
    "@types/stack-trace": "0.0.29",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "eslint": "6.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prefer-arrow": "1.1.6",
    "mocha": "6.2.2",
    "ts-node": "8.5.2",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.2"
  }
}
